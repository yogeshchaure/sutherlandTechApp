<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="statisticalARDataBTF">
    <default-activity>executeViewCriteria</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <view id="StatisticalDataAR">
      <page>/jsff/StatisticalDataAR.jsff</page>
    </view>
    <method-call id="executeViewCriteria">
      <method>#{bindings.executeViewCriteria.execute}</method>
      <outcome>
        <fixed-outcome>executeViewCriteria</fixed-outcome>
      </outcome>
    </method-call>
    <control-flow-rule id="__1">
      <from-activity-id>executeViewCriteria</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>executeViewCriteria</from-outcome>
        <to-activity-id>StatisticalDataAR</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
