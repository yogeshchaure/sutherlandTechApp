              <f:selectItems value="#{bindings.SectionApplicability.items}" id="si2"/>
              <f:validator binding="#{bindings.SectionApplicability.validator}"/>
            </af:selectOneChoice>
            <af:selectOneChoice value="#{bindings.IsPanTrcAvailable.inputValue}"
