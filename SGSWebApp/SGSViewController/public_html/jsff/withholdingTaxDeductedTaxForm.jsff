              <f:selectItems value="#{bindings.ServiceProvider.items}" id="si6"/>
              <f:validator binding="#{bindings.ServiceProvider.validator}"/>
            </af:selectOneChoice>
            <af:selectOneChoice value="#{bindings.PointOfDeduction.inputValue}"
