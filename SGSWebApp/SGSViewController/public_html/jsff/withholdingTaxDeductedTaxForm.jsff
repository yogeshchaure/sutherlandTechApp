              <f:selectItems value="#{bindings.Currency.items}" id="si1"/>
              <f:validator binding="#{bindings.Currency.validator}"/>
            </af:selectOneChoice>
            <af:inputText value="#{bindings.ThresholdLimit.inputValue}" label="Threshold Limit"
